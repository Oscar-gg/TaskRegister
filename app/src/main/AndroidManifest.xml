<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.TaskRegister">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoIA">
        <activity
            android:name="com.example.TaskRegister.GeneralInformation"
            android:parentActivityName="com.example.TaskRegister.MainActivity" />
        <activity
            android:name="com.example.TaskRegister.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.TaskRegister.MainActivity" />
        <activity
            android:name="com.example.TaskRegister.RegisterData"
            android:parentActivityName="com.example.TaskRegister.MainActivity" />
        <activity
            android:name="com.example.TaskRegister.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProyectoIA.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>